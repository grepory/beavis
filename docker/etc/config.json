{
  "db-spec": {
    "classname":"org.postgresql.Driver",
    "subprotocol":"postgresql",
    "host":"{{DB_HOST}}",
    "port":"{{DB_PORT}}",
    "subname":"{{DB_NAME}}",
    "user":"{{DB_USER}}",
    "password":"{{DB_PASS}}",
    "max-conns":6,
    "min-conns":1,
    "init-conns":1
  },
  "secret":"/beavis/etc/vape.key",
  "server":{
    "port":8081
  },
  "nsq":{
    "lookup":"{{NSQ_LOOKUP}}",
    "produce":"{{NSQ_PRODUCE}}"
  }
}